{
  "name": "@ldesign/web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "LDesign Web 管理界面 - Vue3 + Vite + Naive UI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/post-build.js",
    "build:lib": "tsup",
    "build:check": "vue-tsc --noEmit && vite build && node scripts/post-build.js",
    "build:prod": "vue-tsc --noEmit && vite build && node scripts/post-build.js",
    "preview": "vite preview",
    "start": "vite preview",
    "start:prod": "vite preview --port 5173 --host 0.0.0.0"
  },
  "dependencies": {
    "@vicons/antd": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "axios": "^1.6.0",
    "lucide-vue-next": "^0.548.0",
    "naive-ui": "^2.43.1",
    "pinia": "^2.1.7",
    "vue": "^3.5.22",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.21",
    "vue-tsc": "^1.8.27"
  }
}
